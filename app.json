{
  "expo": {
    "name": "BRICOLLANO",
    "slug": "bricollano",
    "version": "1.0.0",
    "scheme": "bricollano",
    "orientation": "portrait",
    "icon": "./assets/logos/BRICOLLANO-logo-black.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logos/BRICOLLANO-logo-blue.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/BRICOLLANO-logo-black.png",
        "backgroundColor": "transparent"
      }
    },
    "web": {
      "favicon": "./assets/logos/BRICOLLANO-logo-black.png"
    },
    "plugins": [
      "expo-web-browser",
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "32be857b-e444-4b17-b2bd-d8b94d70cfb6"
      }
    },
    "owner": "amine300"
  }
}
