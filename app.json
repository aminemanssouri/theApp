{
  "expo": {
    "name": "BRICOLLANO",
    "slug": "bricollano",
    "version": "1.0.0",
    "scheme": "bricollano",
    "orientation": "portrait",
    "icon": "./assets/logos/BRICOLLANO-logo-black.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logos/BRICOLLANO-logo-blue.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/BRICOLLANO-logo-black.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "bricollano.info.com"
    },
    "web": {
      "favicon": "./assets/logos/BRICOLLANO-logo-black.png"
    },
    "plugins": [
      "expo-web-browser",
      "expo-notifications",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.bricollano.info.com",
          "enableGooglePay": true
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "0237e1bf-7d62-4aa8-ab62-e613c6f36607"
      }
    },
    "owner": "bricollano"
  }
}
